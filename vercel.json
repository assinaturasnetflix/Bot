{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(how-to-play|history|game|forgot-password|lobby|login|privacy|profile|public-profile|ranking|register|responsible-gaming|support|terms|wallet|admin-dashboard|admin-login|admin-settings|admin-transactions|admin-users|landing|index)\\.html",
      "dest": "/$1.html"
    },
    {
      "src": "/",
      "dest": "/landing.html"
    }
  ]
}